{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "tomi",
  "version": "0.1.0",
  "identifier": "com.brielle.tomi",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "tomi",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.icns", "icons/icon.ico", "icons/32x32.png", "icons/128x128.png"
    ]
  }
}
